              .row
                span
                  i.fab.fa-twitter
                p 90K Followers
                a.noevent-btn(href="#") Follow
              .row
                span
                  i.fab.fa-facebook-f
                p 2M Like
                a.noevent-btn(href="#") Like
              .row
                span
                  i.fab.fa-youtube
                p 1M Subs
                a.noevent-btn(href="#") Subscribe
              .row
                span
                  i.fab.fa-linkedin
                p 70K Followers
                a.noevent-btn(href="#") Follow

