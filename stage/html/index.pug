                span.noevent-btn 8.9mb
              .row
                img(src="assets/images/files/dll.svg", alt="File Image")
                .title
                  h4 My-DLL-File.pdf
                  span Admin
                span.noevent-btn 4.9mb
              .row
                img(src="assets/images/files/eps.svg", alt="File Image")
